{
  "name": "notifique-me-admin",
  "version": "1.2.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:client": "vite --config client/vite.config.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build": "npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npx tsc --noEmit",
    "format": "npx prettier --write .",
    "test": "npx vitest run",
    "db:push": "npx drizzle-kit generate && npx drizzle-kit migrate",
    "db:init": "npx tsx scripts/init-db.ts",
    "db:migrate": "npx tsx scripts/init-db.ts",
    "setup": "npm install && npm run db:init && npm run check",
    "clean": "rimraf dist node_modules/.vite",
    "worker": "npx tsx scripts/process-schedules.js --loop"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",

  }
}
