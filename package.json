{
  "name": "notifique-me-admin",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev:client": "vite --config client/vite.config.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "esbuild server/_core/index.ts --platform=node --bundle --format=esm --outfile=dist/index.js --external:lightningcss --external:@tailwindcss/vite --external:postgres --external:@aws-sdk/client-s3 --external:@aws-sdk/s3-request-presigner",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "db:init": "tsx scripts/init-db.ts",
    "db:migrate": "tsx scripts/init-db.ts",
    "setup": "npm install && npm run db:init && npm run check",
    "clean": "rimraf dist node_modules/.vite",
    "worker": "tsx scripts/process-schedules.js --loop"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
